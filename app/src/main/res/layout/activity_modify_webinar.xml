<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/modifyWebinarF"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".home.ModifyWebinar">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <TextView
            android:id="@+id/createWebinar"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            style="@android:style/Widget.Holo.Button.Borderless.Small"
            android:gravity="left"
            android:layout_marginTop="10dp"
            android:fontFamily="@font/playfairdisplayregular"
            android:text="Create webinar"
            android:textColor="@color/black"
            android:textSize="25dp" />

        <TextView
            android:id="@+id/updateWebinar"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            style="@android:style/Widget.Holo.Button.Borderless.Small"
            android:gravity="left"
            android:layout_marginTop="10dp"
            android:fontFamily="@font/playfairdisplayregular"
            android:text="Edit webinar"
            android:textColor="@color/black"
            android:textSize="25dp" />

        <Button
            android:id="@+id/btn_backtoProfile"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:layout_marginTop="20dp"
            android:fontFamily="@font/playfairdisplayregular"
            android:textColor="@color/black"
            android:text="Back" />
    </LinearLayout>

</FrameLayout>